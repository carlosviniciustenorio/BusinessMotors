{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning", 
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authorization": "Debug"
    }
  },
  "ApiSettings": {
    "Cache": {
      "InstanceName": "CManager",
      "Configuration": "localhost:6379"
    },
    "ElasticSearch":{
      "Endpoint": "http://localhost:9200",
      "Index": ["cmanager", "cars"]
    },
    "ConnectionStringDB":"Server=db;Port=3306;Database=mydatabase;Uid=myuser;Pwd=mypassword"
  },
  "JwtOptions": {
    "Issuer": "http://localhost",
    "Audience": "Audience",
    "SecurityKey": "B443833D-22D4-436A-A942-0933BF16B248AE2AEF1A-327E-495D-8AFB-925F035DA686",
    "AccessTokenExpiration": 3600,
    "RefreshTokenExpiration": 10800,
    "PrivateKeyPath": "Keys/private.key",
    "PublicKeyPath": "Keys/public.key"
  },
  "Google": {
    "Authority": "",
    "Issuer": "",
    "Audience": ""
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 5
      },
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 100
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 1000
      },
      {
        "Endpoint": "*",
        "Period": "1d",
        "Limit": 10000
      }
    ]
  }
}
